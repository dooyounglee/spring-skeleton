<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
                        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.doo.skeleton.db.mybatis.MariaMapperTest">

	<insert id="insertTest" parameterType="Map">
		INSERT INTO MARIADTO
		VALUES (#{id})
	</insert>
	
	<delete id="deleteTest" parameterType="Map">
		DELETE FROM MARIADTO
		 WHERE ID = #{id}
	</delete>
	
	<select id="selectTest" parameterType="Map" resultType="com.doo.skeleton.example.dto.db.first.MariaDto">
		SELECT ID
		  FROM MARIADTO
		 WHERE ID > #{prefix}
	</select>
	
</mapper>